{
  "name": "vite-project",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "vite --mode dev --open",
    "build:test": "vue-tsc -b && vite build --mode test",
    "build:prod": "vue-tsc -b && vite build --mode prod",
    "preview": "vite preview",
    "commit": "cz",
    "lint": "stylelint \"**/*.{css,scss,less,vue}\" --config stylelint.config.js --fix && eslint --fix && prettier . --write"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.12",
    "@vueuse/core": "^13.3.0",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "element-plus": "^2.9.11",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.2",
    "storagens": "^1.0.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.27.0",
    "@eslint/json": "^0.12.0",
    "@tailwindcss/vite": "^4.1.8",
    "@types/node": "^22.15.26",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.4.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "postcss": "^8.5.4",
    "postcss-html": "^1.8.0",
    "prettier": "3.5.3",
    "rollup-plugin-visualizer": "^6.0.1",
    "sass-embedded": "^1.89.0",
    "stylelint": "^16.19.1",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-scss": "^6.12.0",
    "tailwindcss": "^4.1.8",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.0",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vue-eslint-parser": "^10.0.0",
    "vue-tsc": "^2.2.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  }
}
